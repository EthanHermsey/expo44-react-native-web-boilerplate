{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "*": [
                "src/*"
            ],
            "assets/*": [
                "./src/assets/*"
            ],
            "components/*": [
                "./src/components/*"
            ],
            "hooks/*": [
                "./src/hooks/*"
            ],
            "navigation/*": [
                "./src/navigation/*"
            ],
            "pages/*": [
                "./src/pages/*"
            ],
            "providers/*": [
                "./src/providers/*"
            ],
            "screens/*": [
                "./src/screens/*"
            ],
            "store/*": [
                "./src/store/*"
            ],
            "styles/*": [
                "./src/styles/*"
            ],
            "utils/*": [
                "./src/utils/*"
            ],
        }
    }
}